FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues ( volScalarFieldValue porosity 1 );

regions
(

 boxToCell {
   box (-100 -100 -100) (100 1e-5 100);
   fieldValues ( volScalarFieldValue porosity 0.5);
 }

 );

